<h1>Books</h1>
  <table>
    <thead>
    <%@books.each do |book| %>
        <p>Title</p>
        <p>Body</p>
        <p colspn="3"></p>
      </tr>
    </thead>
    <% end %>
  </table>

  <h2>New book</h2>
  <%= form_with model:@book, local:true do |f| %>

  　<h4>Title</h4>
  　<%= f.text_field :title %>

  　<h4>Body</h4>
  　<%= f.text_area :body %>

  　<%= f.submit 'Create Book' %>

  <% end %>